{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/account/(.*)": "<rootDir>/../libs/account/src/$1",
    "@app/account": "<rootDir>/../libs/account/src",
    "@app/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@app/prisma": "<rootDir>/../libs/prisma/src",
    "@app/redis/(.*)": "<rootDir>/../libs/redis/src/$1",
    "@app/redis": "<rootDir>/../libs/redis/src"
  }
}