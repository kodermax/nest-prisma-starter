{
  "version": 2,
  "outputDirectory": "dist",
  "buildCommand": "yarn build",
  "functions": {
    "main.js": {
      "memory": 3008,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
