{
  "name": "jewsource",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "admin": "turbo run dev --scope=admin",
    "api": "turbo run dev --scope=api",
    "api:build": "turbo run dev --scope=api",
    "web": "turbo run dev --scope=web",
    "build": "turbo run build",
    "build:admin": "turbo run build --scope=admin",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "^1.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
